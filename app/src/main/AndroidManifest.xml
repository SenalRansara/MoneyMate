<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.moneymate">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/moneyapplogo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MoneyMate">
        <activity android:name=".Update_Account"
           android:parentActivityName=".income"
            />
        <activity
            android:name=".new_Account"
            android:label="New Account"
            android:parentActivityName=".income" />
        <activity android:name=".income"
            android:label =" MoneyMate">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>